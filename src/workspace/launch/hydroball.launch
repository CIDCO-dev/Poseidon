<launch>

<arg name="sonarip" value="192.168.0.88"/>
<arg name="sonarport" value="5000"/>
<arg name="gpsip" value="_host:=127.0.0.1"/>
<arg name="gpsport" value="_port:=2947"/>
<arg name="loggerPath" value="/home/ubuntu/Poseidon/www/webroot/record/"/>
<arg name="portPath1" value="/dev/ttyACM0"/>


<node pkg="logger_binary" name="logger_binary" type="logger_binary" output="screen" args="$(arg loggerPath)"/>

<node pkg="logger_text" name="logger_text" type="logger_text" output="screen" args="$(arg loggerPath)"/>

<node pkg="gpsd_client" name="GPS" type="gpsd_client" output="screen" args="$(arg gpsip) $(arg gpsport)"/> 

<node pkg="sonar_nmea_0183_tcp_client" name="Sonar" type="sonar_nmea_0183_tcp_client" output="screen" args="$(arg sonarip) $(arg sonarport)"/>

<node pkg="imu_dummy" name="IMU" type="imu_dummy" output="screen"/>

<node pkg="raspberrypi_vitals" name="Vitals" type="raspberrypi_vitals" output="screen"/>
  
<node pkg="state_controller" name="state_controller" type="state_controller" output="screen"/>

<!--<node pkg="catarob_control" name="catarob_control" type="catarob_control" output="screen"/>-->

<node pkg="catarob_control_websocket" name="catarob_control_websocket" type="catarob_control_websocket" args="$(arg loggerPath)"/>


<node pkg="hydroball_data_websocket" name="hydroball_data_websocket" type="hydroball_data_websocket" args="$(arg loggerPath)"/>

<!--<node pkg="gnss_zed-f9p" name="gnss_zed_f9p" type="gnss_zed-f9p" output="screen" args="$(arg loggerPath1) $(arg portPath1)"/>-->


</launch>
